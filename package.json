{
  "name": "@beautywe/plugin-event",
  "version": "0.0.1",
  "description": "Event plugin for beautywe",
  "main": "dist/event.js",
  "scripts": {
    "test": "node_modules/.bin/nyc ava test/**/*.test.js",
    "test-report": "node_modules/.bin/nyc report --reporter=html",
    "build-pro": "rm -rf ./dist && node_modules/.bin/babel src -d dist",
    "build-watch": "node_modules/.bin/babel src --watch -d dist",
    "code-check": "node_modules/.bin/eslint src/**",
    "code-fix": "node_modules/.bin/eslint --fix src/**",
    "lint": "git diff --cached --name-only | grep -E '\\.(js|jsx)$' | xargs eslint",
    "commitmsg": "validate-commit-msg",
    "commit": "git cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beautywe/beautywe-plugin-event.git"
  },
  "keywords": [],
  "author": "JerryC (huangjerryc@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beautywe/beautywe-plugin-event/issues"
  },
  "homepage": "https://github.com/beautywe/beautywe-plugin-event#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@beautywe/core": "0.0.2",
    "ava": "^1.4.1",
    "babel-eslint": "^9.0.0",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-istanbul": "^5.1.0",
    "commitizen": "^3.0.5",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "nyc": "^13.1.0",
    "pre-commit": "^1.2.2",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
